//读取两个数的值，用实数显示他们的和、差、积、商。
#include<stdio.h>
int main()
{
	double vx, vy;
	puts("请输入两个数。");
	printf("实数vx:");
	scanf("%lf", &vx);
	printf("实数vy:");
	scanf("%lf", &vy);

	printf("vx + vy = %f\n", vx + vy);
	printf("vx - vy = %f\n", vx - vy);
	printf("vx * vy = %f\n", vx * vy);
	printf("vx / vy = %f\n", vx / vy);

	return 0;
}
